{
  "manifest_version": 2,
  "name": "New Tab",
  "description": "A custom new tab page.",
  "version": "1.0",
  "developer": {
    "name": "Navinn Ravindaran",
    "url": "https://github.com/navn-r/newtab"
  },
  "permissions": ["webNavigation", "browserSettings"],
  "chrome_url_overrides": { "newtab": "index.html" },
  "browser_specific_settings": {
    "gecko": {
      "id": "newtab@navn.me",
      "strict_min_version": "42.0"
    }
  }
}
